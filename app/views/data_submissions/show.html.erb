<h3>Submission for <%= @data_submission.work_request.scheme_name %>, ID: <%= @data_submission.work_request.id %></h3>

  <table class="table-bordered table-striped mb-2">
    <colgroup>
      <col span="1" style="width: 20%;">
      <col span="3">
   </colgroup>
    <tr>
      <td><%= "Data location" %></td>
      <td><%= @data_submission.data_location%></td>
    </tr>
    <tr>
      <td><%= "Source data" %></td>
      <td><%= @data_submission.source_data %></td>
    </tr>
    <tr>
      <td><%= "Working data" %></td>
      <td><%= @data_submission.working_data %></td>
    </tr>
    <tr>
      <td><%= "Final data" %></td>
      <td><%= @data_submission.final_data %></td>
    </tr>
    <tr>
      <td><%= "Manipulation summary" %></td>
      <td><%= @data_submission.manipulation_summary %></td>
    </tr>
    <tr>
      <td><%= "Data specification" %></td>
      <td><%= @data_submission.data_spec %></td>
    </tr>
    <tr>
      <td><%= "Duplicate members" %></td>
      <td><%= @data_submission.duplicate_members %></td>
    </tr>
  </table>
<%= link_to "Submit Data", edit_data_submission_path(@data_submission), class: "btn btn-sm btn-dark mb-2" %>
<%= link_to "Cancel", :back, class: "btn btn-dark" %>
